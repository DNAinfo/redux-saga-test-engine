{
  "name": "redux-saga-test-engine",
  "version": "2.0.2",
  "description": "Redux saga test helper",
  "engines": {
    "node": ">=6.0.0"
  },
  "engine": {
    "node": ">=6.0.0"
  },
  "main": "build/index.js",
  "repository": "DNAinfo/redux-saga-test-engine",
  "scripts": {
    "start": "npm install",
    "test": "ava tests/",
    "test:watch": "npm test -- --watch",
    "lint": "eslint ./src ./tests",
    "build": "rimraf build && cross-env BABEL_ENV=es babel src -d build --ignore 'tests'",
    "prepublish": "npm run build"
  },
  "keywords": [
    "redux",
    "redux-saga",
    "test"
  ],
  "author": "DNAinfo",
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "cross-env": "^4.0.0",
    "eslint": "^3.13.0",
    "redux-saga": "^0.14.1",
    "rimraf": "^2.6.1"
  }
}
